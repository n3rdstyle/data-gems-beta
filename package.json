{
  "name": "data-gems-browser-extension",
  "version": "0.1.0",
  "description": "Data Gems Browser Extension with Design System",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:engine && npm run build:offscreen",
    "build:engine": "node build-engine.js",
    "build:offscreen": "node build-offscreen.js",
    "lint:css": "stylelint \"**/*.css\" --ignore-path .stylelintignore",
    "lint:css:fix": "stylelint \"**/*.css\" --fix --ignore-path .stylelintignore",
    "lint:css:report": "stylelint \"**/*.css\" --ignore-path .stylelintignore --formatter verbose"
  },
  "devDependencies": {
    "esbuild": "^0.27.0",
    "stylelint": "^16.0.0",
    "stylelint-config-standard": "^36.0.0"
  },
  "keywords": [
    "browser-extension",
    "design-system",
    "chrome-extension"
  ],
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "dexie": "^4.2.1",
    "rxdb": "^16.20.0",
    "rxjs": "^7.8.2"
  }
}
